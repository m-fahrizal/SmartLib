<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".History">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:background="@drawable/column2"
            android:backgroundTint="@color/ColorWhiteYellow">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/iconback"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="16dp"
                android:id="@+id/backhistory"
                />

            <TextView
                android:id="@+id/history"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/column2"
                android:fontFamily="@font/poppinsbold"
                android:paddingLeft="120dp"
                android:paddingTop="15dp"
                android:text="History"
                android:textColor="@color/ColorRed_1"
                android:textSize="20dp" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="80dp"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/historyList1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="20dp"
                android:background="@drawable/column3"
                android:backgroundTint="@color/ColorWhiteYellow"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/img_book1"
                    android:layout_width="50dp"
                    android:layout_height="93dp"
                    android:layout_marginLeft="18dp"
                    android:src="@drawable/image1" />

                <TextView
                    android:id="@+id/txt_book1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/column3"
                    android:fontFamily="@font/poppinsbold"
                    android:paddingLeft="10dp"
                    android:paddingTop="35dp"
                    android:paddingRight="8dp"
                    android:text="Modular Design Frameworks"
                    android:textColor="@color/ColorRed_1"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/historyList2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="20dp"
                android:layout_marginRight="20dp"
                android:background="@drawable/column3"
                android:backgroundTint="@color/ColorWhiteYellow"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/img_book2"
                    android:layout_width="50dp"
                    android:layout_height="93dp"
                    android:layout_marginLeft="18dp"
                    android:src="@drawable/image12" />

                <TextView
                    android:id="@+id/txt_book2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/column3"
                    android:fontFamily="@font/poppinsbold"
                    android:paddingLeft="10dp"
                    android:paddingTop="25dp"
                    android:paddingRight="8dp"
                    android:text="Warehouses information system design and development"
                    android:textColor="@color/ColorRed_1"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/historyList3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="20dp"
                android:layout_marginRight="20dp"
                android:background="@drawable/column3"
                android:backgroundTint="@color/ColorWhiteYellow"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/img_book3"
                    android:layout_width="50dp"
                    android:layout_height="93dp"
                    android:layout_marginLeft="18dp"
                    android:src="@drawable/image3" />

                <TextView
                    android:id="@+id/txt_book3"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/column3"
                    android:fontFamily="@font/poppinsbold"
                    android:paddingLeft="10dp"
                    android:paddingTop="25dp"
                    android:paddingRight="8dp"
                    android:text="PostgreSQL 14 Administration Cookbook"
                    android:textColor="@color/ColorRed_1"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/historyList4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="20dp"
                android:layout_marginRight="20dp"
                android:background="@drawable/column3"
                android:backgroundTint="@color/ColorWhiteYellow"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/img_book4"
                    android:layout_width="50dp"
                    android:layout_height="93dp"
                    android:layout_marginLeft="18dp"
                    android:src="@drawable/image9" />

                <TextView
                    android:id="@+id/txt_book4"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/column3"
                    android:fontFamily="@font/poppinsbold"
                    android:paddingLeft="10dp"
                    android:paddingTop="5dp"
                    android:paddingRight="8dp"
                    android:text="Orientation-based Ant colony algorithm for synthesizing the test scenarios in UML activity diagram"
                    android:textColor="@color/ColorRed_1"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/historyList5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="20dp"
                android:layout_marginRight="20dp"
                android:background="@drawable/column3"
                android:backgroundTint="@color/ColorWhiteYellow"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/img_book5"
                    android:layout_width="50dp"
                    android:layout_height="93dp"
                    android:layout_marginLeft="18dp"
                    android:src="@drawable/image5" />

                <TextView
                    android:id="@+id/txt_book5"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/column3"
                    android:fontFamily="@font/poppinsbold"
                    android:paddingLeft="10dp"
                    android:paddingTop="35dp"
                    android:paddingRight="8dp"
                    android:text="Industrial Design of Experiments"
                    android:textColor="@color/ColorRed_1"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/historyList6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="20dp"
                android:layout_marginRight="20dp"
                android:background="@drawable/column3"
                android:backgroundTint="@color/ColorWhiteYellow"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/img_book6"
                    android:layout_width="50dp"
                    android:layout_height="93dp"
                    android:layout_marginLeft="18dp"
                    android:src="@drawable/image6" />

                <TextView
                    android:id="@+id/txt_book6"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/column3"
                    android:fontFamily="@font/poppinsbold"
                    android:paddingLeft="10dp"
                    android:paddingTop="20dp"
                    android:paddingRight="8dp"
                    android:text="Learning PHP, MySQL and JavaScript"
                    android:textColor="@color/ColorRed_1"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/historyList7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="20dp"
                android:layout_marginRight="20dp"
                android:background="@drawable/column3"
                android:backgroundTint="@color/ColorWhiteYellow"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/img_book7"
                    android:layout_width="50dp"
                    android:layout_height="93dp"
                    android:layout_marginLeft="18dp"
                    android:src="@drawable/image7" />

                <TextView
                    android:id="@+id/txt_book7"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/column3"
                    android:fontFamily="@font/poppinsbold"
                    android:paddingLeft="10dp"
                    android:paddingTop="5dp"
                    android:paddingRight="8dp"
                    android:text="Model-based testing using UML activity diagrams: A systematic mapping study"
                    android:textColor="@color/ColorRed_1"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/historyList8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="20dp"
                android:layout_marginRight="20dp"
                android:background="@drawable/column3"
                android:backgroundTint="@color/ColorWhiteYellow"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/img_book8"
                    android:layout_width="50dp"
                    android:layout_height="93dp"
                    android:layout_marginLeft="18dp"
                    android:src="@drawable/image8" />

                <TextView
                    android:id="@+id/txt_book8"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/column3"
                    android:fontFamily="@font/poppinsbold"
                    android:paddingLeft="10dp"
                    android:paddingTop="5dp"
                    android:paddingRight="8dp"
                    android:text="Automatic extraction of product line architecture and feature models from UML class diagram variants"
                    android:textColor="@color/ColorRed_1"
                    android:textSize="14sp" />
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>
</ScrollView>