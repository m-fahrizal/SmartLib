<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".JournalList">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:background="@drawable/column2"
        android:backgroundTint="@color/ColorWhiteYellow">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/iconback"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="15dp"
            android:id="@+id/back_journal1"
            />

        <TextView
            android:id="@+id/edt_journal1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/column2"
            android:fontFamily="@font/poppinsbold"
            android:paddingLeft="120dp"
            android:paddingTop="15dp"
            android:text="Journal"
            android:textColor="@color/ColorRed_1"
            android:textSize="20dp" />


    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="70dp"
        android:orientation="vertical">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="35dp"
            android:text="2017"
            android:textColor="@color/ColorGreen"
            android:textSize="20dp"
            android:textStyle="bold" />

        <LinearLayout
            android:id="@+id/journal1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/column3"
            android:backgroundTint="@color/ColorWhiteYellow"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/img_journal1"
                android:layout_width="50dp"
                android:layout_height="93dp"
                android:layout_marginLeft="18dp"
                android:src="@drawable/image12" />

            <TextView
                android:id="@+id/txt_journal1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="20dp"
                android:background="@drawable/column3"
                android:fontFamily="@font/poppinsbold"
                android:gravity="center"
                android:paddingRight="8dp"
                android:text="Warehouses information system design and development"
                android:textColor="@color/ColorRed_1"
                android:textSize="14sp" />
        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="35dp"
            android:layout_marginTop="10dp"
            android:text="2018"
            android:textColor="@color/ColorGreen"
            android:textSize="20dp"
            android:textStyle="bold" />

        <LinearLayout
            android:id="@+id/journal2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/column3"
            android:backgroundTint="@color/ColorWhiteYellow"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/img_journal2"
                android:layout_width="50dp"
                android:layout_height="93dp"
                android:layout_marginLeft="18dp"
                android:src="@drawable/image10" />

            <TextView
                android:id="@+id/txt_journal2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="20dp"
                android:background="@drawable/column3"
                android:fontFamily="@font/poppinsbold"
                android:gravity="center"
                android:paddingRight="8dp"
                android:text="Tool Support to Automate Transformations from SBVR to UML Use Case Diagram"
                android:textColor="@color/ColorRed_1"
                android:textSize="14sp" />
        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="35dp"
            android:layout_marginTop="10dp"
            android:text="2019"
            android:textColor="@color/ColorGreen"
            android:textSize="20dp"
            android:textStyle="bold" />

        <LinearLayout
            android:id="@+id/journal3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/column3"
            android:backgroundTint="@color/ColorWhiteYellow"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/img_journal3"
                android:layout_width="50dp"
                android:layout_height="93dp"
                android:layout_marginLeft="18dp"
                android:src="@drawable/image7" />

            <TextView
                android:id="@+id/txt_journal3"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="20dp"
                android:background="@drawable/column3"
                android:fontFamily="@font/poppinsbold"
                android:gravity="center"
                android:paddingRight="8dp"
                android:text="Model-based testing using UML activity diagrams: A systematic mapping study"
                android:textColor="@color/ColorRed_1"
                android:textSize="14sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/journal4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/column3"
            android:backgroundTint="@color/ColorWhiteYellow"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/img_journal4"
                android:layout_width="50dp"
                android:layout_height="93dp"
                android:layout_marginLeft="18dp"
                android:src="@drawable/image8" />

            <TextView
                android:id="@+id/txt_journal4"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="20dp"
                android:background="@drawable/column3"
                android:fontFamily="@font/poppinsbold"
                android:gravity="center"
                android:paddingRight="8dp"
                android:text="Automatic extraction of product line architecture and feature models from UML class diagram variants"
                android:textColor="@color/ColorRed_1"
                android:textSize="14sp" />
        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="35dp"
            android:layout_marginTop="10dp"
            android:text="2020"
            android:textColor="@color/ColorGreen"
            android:textSize="20dp"
            android:textStyle="bold" />

        <LinearLayout
            android:id="@+id/journal5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/column3"
            android:backgroundTint="@color/ColorWhiteYellow"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/img_journal5"
                android:layout_width="50dp"
                android:layout_height="93dp"
                android:layout_marginLeft="18dp"
                android:src="@drawable/image9" />

            <TextView
                android:id="@+id/txt_journal5"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="20dp"
                android:background="@drawable/column3"
                android:fontFamily="@font/poppinsbold"
                android:gravity="center"
                android:paddingRight="8dp"
                android:text="Orientation-based Ant colony algorithm for synthesizing the test scenarios in UML activity diagram"
                android:textColor="@color/ColorRed_1"
                android:textSize="14sp"
                />
        </LinearLayout>

    </LinearLayout>

</RelativeLayout>